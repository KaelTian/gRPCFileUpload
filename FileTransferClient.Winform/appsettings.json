{
  "Application": {
    "Title": "gRPC 客户端应用程序",
    "Theme": "Dark"
  },
  "GrpcSettings": {
    "Services": {
      "FileTransferClient": {
        "ServerUrl": "http://localhost:5173",
        "TimeoutSeconds": 30,
        "MaxRetryAttempts": 3,
        "RetryDelayMilliseconds": 1000,
        "EnableCompression": false
      },
      "HealthCheckClient": {
        "ServerUrl": "http://localhost:5173",
        "TimeoutSeconds": 20,
        "MaxRetryAttempts": 2,
        "RetryDelayMilliseconds": 500,
        "EnableCompression": false
      }
    },
    "GlobalSettings": {
      "KeepAlivePingDelay": 60,
      "KeepAlivePingTimeout": 5
      //"MaxReceiveMessageSize": 4194304,
      //"MaxSendMessageSize": 4194304
    }
  }
}